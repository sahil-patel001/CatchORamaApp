{
  "name": "product-ecosystem-backend",
  "version": "1.0.0",
  "description": "Node.js backend for Product Ecosystem Management Platform",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seeders/index.js",
    "seed:fresh": "node seeders/index.js --fresh",
    "fix:categories": "node scripts/fix-category-duplicates.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "migrate": "node -r dotenv/config scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "migrate:status": "node scripts/migrate.js status",
    "migrate:list": "node scripts/migrate.js list",
    "setup-env": "node scripts/setup-env.js",
    "validate-env": "node scripts/validate-env.js",
    "generate-env": "node -e \"import('./config/environment.js').then(m => m.generateEnvTemplate())\"",
    "test-email-templates": "node scripts/test-email-templates.js",
    "validate-email-templates": "node scripts/test-email-templates.js validate",
    "email-template-report": "node scripts/test-email-templates.js report",
    "test-low-stock-notifications": "node scripts/test-low-stock-notifications.js",
    "test-new-order-notifications": "node scripts/test-new-order-notifications.js",
    "test-cubic-volume-notifications": "node scripts/test-cubic-volume-notifications.js",
    "test-all-email-templates": "node scripts/test-all-email-templates.js",
    "email-template-features": "node scripts/test-all-email-templates.js features",
    "test-notification-integrations": "node scripts/test-notification-integrations.js",
    "test-vendor-threshold-management": "node scripts/test-vendor-threshold-management.js",
    "test-dual-delivery-system": "node scripts/test-dual-delivery-system.js",
    "test-performance": "node scripts/test-performance-optimizations.js",
    "clear-rate-limits": "node scripts/clear-rate-limits.js"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.883.0",
    "@sendgrid/mail": "^8.1.5",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsbarcode": "^3.12.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "socket.io": "^4.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/node": "^24.7.2",
    "concurrently": "^9.1.2",
    "jest": "^30.1.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "mongoose",
    "passport",
    "jwt",
    "zod",
    "api"
  ],
  "author": "Product Ecosystem Team",
  "license": "MIT"
}
